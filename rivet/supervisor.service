[Unit]
Description=Agent Factory Slack Supervisor
After=network.target

[Service]
Type=simple
User=root
WorkingDirectory=/root/Agent-Factory
ExecStart=/usr/bin/python3 -m uvicorn agent_factory.observability.server:create_app --factory --host 0.0.0.0 --port 3001
EnvironmentFile=/root/Agent-Factory/.env
Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target
